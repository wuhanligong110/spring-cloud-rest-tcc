<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

    <classPathEntry location=""/>

    <context id="context" targetRuntime="MyBatis3Simple">
        <commentGenerator>
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection userId="chris" password="123123" driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:solar_mysql:3306:///membership?serverTimezone=UTC"/>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.github.prontera.domain" targetProject="${dao.target.dir}">
            <property name="enableSubPackages" value="true"/>
            <!--<property name="trimStrings" value="true"/>-->
            <property name="rootClass" value="com.github.prontera.model.BasicDomain"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.github.prontera.persistence" targetProject="${dao.target.dir}">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="" type="XMLMAPPER" targetProject=".">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>

        <table schema="" tableName="" enableCountByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableUpdateByExample="false"/>
    </context>
</generatorConfiguration>